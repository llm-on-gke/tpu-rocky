{
    "Name": "tpu-vm-image",
    "Project": "northam-ce-mlai-tpu",
    "Zone": "us-central1-a",
    "GCSPath": "gs://rick-rocky-tpu-image/daisy/tpu_vm_images",
    "Vars": {
      "workflow_root": {
        "Value": "/workflows",
        "Description": "Root of github workflows, defaults to /workflows in the container."
      },
      "source_image": {
        "Value": "projects/rocky-linux-cloud/global/images/family/rocky-linux-9-optimized-gcp",
        "Description": "URI of the source image to be modified by this workflow"
      },
      "variant": {
        "Value": "rocky",
        "Description": "An optional variant string to be added to the generated image's name"
      },
      "custom_kernel_params": {
        "Value": "",
        "Description": "An optional string with kernel cmdlines. These are appended to the image's boot entries. NOTE: These params can modify a VM's behavior significantly, use this option with caution and always follow with adequate testing."
      },
      "disable_unattended_upgrades": {
        "Value": "FALSE",
        "Description": "If set to 'TRUE' disable unattended-upgrades in 'apt' config."
      },
      "build_date": "${TIMESTAMP}"
    },
    "Sources": {
      "startup_script.sh": "./tpu_vm.sh",
      "validation_script.sh": "./tpu_driver_vm_validation.sh",
      "99-tpu-vfiopci.rules": "./drivers/99-tpu-vfiopci.rules",
      "bind_to_vfio_pci.sh": "./drivers/bind_to_vfio_pci.sh"
    },
    "Steps": {
      "create-disks": {
        "CreateDisks": [
          {
            "Name": "disk-install",
            "SourceImage": "${source_image}",
            "Type": "pd-ssd",
            "SizeGb": "50"
          }
        ]
      },
      "create-inst-install": {
        "CreateInstances": [
          {
            "Name": "inst-install",
            "Disks": [{"Source": "disk-install"}],
            "MachineType": "n2d-standard-32",
            "Scopes": ["https://www.googleapis.com/auth/devstorage.read_write",
              "https://www.googleapis.com/auth/cloudkms"],
            "Metadata": {
              "block-project-ssh-keys": "TRUE",
              "startup-script": "${SOURCE:startup_script.sh}",
              "variant": "${variant}",
              "custom_kernel_params": "${custom_kernel_params}",
              "disable_unattended_upgrades": "${disable_unattended_upgrades}"
            }
          }
        ]
      },
      "wait-for-install": {
        "TimeOut": "120m",
        "waitForInstancesSignal": [
          {
            "Name": "inst-install",
            "SerialOutput": {
              "Port": 1,
              "SuccessMatch": "Preload done.",
              "FailureMatch": "BuildFailed:",
              "StatusMatch": "Status:"
            }
          }
        ]
      },
      "stop-instance": {
        "StopInstances": {
          "Instances":["inst-install"]
        }
      },
      "change-to-validation-script": {
        "UpdateInstancesMetadata": [
          {
            "Instance": "inst-install",
            "Metadata": {
              "startup-script": "${SOURCE:validation_script.sh}"
            }
          }
        ]
      },
      "start-instance": {
        "StartInstances": {
          "Instances":["inst-install"]
        }
      },
      "wait-for-validation": {
        "TimeOut": "30m",
        "waitForInstancesSignal": [
          {
            "Name": "inst-install",
            "SerialOutput": {
              "Port": 1,
              "SuccessMatch": "Validation done.",
              "FailureMatch": "BuildFailed:",
              "StatusMatch": "Status:"
            }
          }
        ]
      },
      "delete-inst-install": {
        "DeleteResources": {
          "Instances": ["inst-install"]
        }
      },
      "create-image": {
        "CreateImages": [
        {
            "Name": "tpu-vm-${variant}-${DATE}",
            "Family": "tpu-vm-${variant}",
            "Project": "northam-ce-mlai-tpu",
            "Description": "Rocky Linux image with tpu driver and VM agents installed.",
            "SourceDisk": "disk-install",
            "NoCleanup": true,
            "ExactName": true,
            "GuestOsFeatures": ["VIRTIO_SCSI_MULTIQUEUE", "UEFI_COMPATIBLE", "SECURE_BOOT"],
            "ShieldedInstanceInitialState": {
              "Dbs": [
                {
                  "FileType": "X509",
                  "Content":""
                },
